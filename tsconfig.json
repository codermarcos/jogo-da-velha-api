{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",                          
    "module": "commonjs",                     
    "outDir": "./dist",                       
    "lib": ["dom", "esnext"],
    "strict": true,                           
    "esModuleInterop": true,  
    "noUnusedLocals": true,                
    "noUnusedParameters": true,            
    "noImplicitReturns": true,             
    "noFallthroughCasesInSwitch": true,    
  }
}
